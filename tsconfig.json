{
  "compilerOptions": {
    "target": "es2022",
    "lib": [
      "es2022",
      "ESNext.Disposable",
    ],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist"
  },
  "include": ["src/**/*.ts"]
}
